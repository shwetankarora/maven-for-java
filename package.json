{
  "name": "maven-for-java",
  "displayName": "Maven for Java",
  "description": "Manage Maven projects, execute goals, generate project from archetype, improve user experience for Java developers.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:maven-for-java.helloWorld"
  ],
